# run container once a week
0 0 * * 0 root echo "docker run -it -e ELASTICSEARCH_ENDPOINT=$ELASTICSEARCH_ENDPOINT -e ELASTICSEARCH_PORT=$ELASTICSEARCH_PORT --rm farooqsadiq/nutch:latest" >> /var/log/cron.log 2>&1
# Don't remove the empty line at the end of this file. It is required to run the cron job
